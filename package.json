{
  "name": "training-schedule-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . && stylelint \"src/**/*.{css,scss}\"",
    "lint:js": "eslint .",
    "lint:css": "stylelint \"src/**/*.{css,scss}\"",
    "lint:fix": "eslint . --fix && stylelint \"src/**/*.{css,scss}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,scss,md}\"",
    "prepare": "husky install",
    "preview": "vite preview",
    "test": "playwright test --config=playwright.config.mjs",
    "test:ui": "playwright test --ui --config=playwright.config.mjs",
    "test:headed": "playwright test --headed --config=playwright.config.mjs",
    "test:debug": "playwright test --debug --config=playwright.config.mjs",
    "test:report": "playwright show-report"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/x-date-pickers": "^8.8.0",
    "dayjs": "^1.11.13",
    "firebase": "^11.10.0",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "react-router-dom": "^7.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@eslint/mcp": "^0.1.1",
    "@playwright/mcp": "^0.0.34",
    "@playwright/test": "^1.54.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-prettier": "^5.0.3",
    "vite": "^7.0.4"
  }
}
